<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cron_update_garista_status" model="ir.cron">
        <field name="name">Update Garista Status</field>
        <field name="model_id" ref="garista.model_garista_garista"/>
        <field name="state">code</field>
        <field name="code">model.update_garista_status()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
